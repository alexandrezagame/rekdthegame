);\n\n  useEffect(() => {\n    if (cart.id) {\n      const generateToken = async () => {\n        try {\n          const token = await commerce.checkout.generateToken(cart.id, {\n            type: 'cart',\n          });\n          console.log('TOKEN', token)\n          setCheckoutToken(token);\n        } catch (error) {\n          if (activeStep !== steps.length) {\n            history.push('/');\n          }\n        }\n      };\n      generateToken();\n    }\n  }, [cart]);\n  const next = (data) => {\n    setShippingData(data);\n\n    nextStep();\n  };\n\n  let Confirmation = () =>\n    order.customer ? (\n      <>\n        <div>\n          <Typography variant=\"h5\">\n            Thank you for your purchase, {order.customer.firstname}{' '}\n            {order.customer.lastname}!\n          </Typography>\n          <Divider className={classes.divider} />\n          <Typography variant=\"subtitle2\">\n            Order ref: {order.customer_reference}\n          </Typography>\n        </div>\n        <br />\n        <Button component={Link} variant=\"outlined\" type=\"button\" to=\"/\">\n          Back to home\n        </Button>\n      </>\n    ) : (\n      <div className={classes.spinner}>\n        <CircularProgress />\n      </div>\n    );\n  if (error) {\n    Confirmation = () => (\n      <>\n        <Typography variant=\"h5\">Error: {error}</Typography>\n        <br />\n        <Button component={Link} variant=\"outlined\" type=\"button\" to=\"/\">\n          Back to home\n        </Button>\n      </>\n    );\n  }\n\n  const Form = () => \n    activeStep === 0 ? (\n      <AddressForm\n        checkoutToken={checkoutToken}\n        nextStep={nextStep}\n        setShippingData={setShippingData}\n        next={next}\n      />\n    ) : (\n      <PaymentForm\n        checkoutToken={checkoutToken}\n        backStep={backStep}\n        nextStep={nextStep}\n        shippingData={shippingData}\n        onCaptureCheckout={onCaptureCheckout}\n      />\n    );\n  \n\n  return (\n    <>\n        <CssBaseline />\n        <div className={classes.toolbar} />\n        <main className={classes.layout}>\n          <Paper className={classes.paper}>\n            <Typography variant=\"h4\" align=\"center\">\n              Checkout\n            </Typography>\n            <Stepper activeStep={activeStep} className={classes.stepper}>\n              {steps.map((label) => (\n                <Step key={label}>\n                  <StepLabel>{label}</StepLabel>\n                </Step>\n              ))}\n            </Stepper>\n            {activeStep === steps.length ? (\n              <Confirmation />\n            ) : (\n              checkoutToken && <Form />\n            )}\n          </Paper>\n        </main>\n      </>\n  )\n};\n\nexport default Checkout;\n",["112","113"],"/Users/jesper/Desktop/rekdthegame/src/components/Cart/styles.js",[],"/Users/jesper/Desktop/rekdthegame/src/components/Hero/styles.js",[],["114","115"],"/Users/jesper/Desktop/rekdthegame/src/components/Products/Products.jsx",[],"/Users/jesper/Desktop/rekdthegame/src/components/CardFlipper/CardFlipper.jsx",["116","117"],"/Users/jesper/Desktop/rekdthegame/src/App.js",[],["118","119"],"/Users/jesper/Desktop/rekdthegame/src/components/CardFlipper/styles.js",[],"/Users/jesper/Desktop/rekdthegame/src/components/CardFlipper/ChallengeCard.jsx",[],"/Users/jesper/Desktop/rekdthegame/src/components/index.js",[],["120","121"],"/Users/jesper/Desktop/rekdthegame/src/components/CardFlipper/SpecialCard.jsx",[],"/Users/jesper/Desktop/rekdthegame/src/components/CardFlipper/PunishmentCard.jsx",[],"/Users/jesper/Desktop/rekdthegame/src/index.js",[],["122","123"],"/Users/jesper/Desktop/rekdthegame/src/components/Banner/Banner.jsx",[],"/Users/jesper/Desktop/rekdthegame/src/components/Products/Product/ProductPage/ProductPage.jsx",["124"],"import { Button } from '@material-ui/core';\nimport React from 'react';\n\nconst ProductPage = ({ onAddToCart }) => {\n  console.log('HELLGSEOKLKSDJGKLSJGL');\n  return (\n    <div>\n      <h1>PRODUCT PAGE</h1>\n    </div>\n  );\n};\n\nexport default ProductPage;\n","/Users/jesper/Desktop/rekdthegame/src/components/Navbar/styles.js",[],"/Users/jesper/Desktop/rekdthegame/src/components/Banner/styles.js",[],"/Users/jesper/Desktop/rekdthegame/src/components/Products/styles.js",[],"/Users/jesper/Desktop/rekdthegame/src/components/Navbar/Navbar.jsx",["125","126"],"import React from 'react';\nimport {\n  AppBar,\n  Toolbar,\n  IconButton,\n  Badge,\n  MenuItem,\n  Menu,\n  Typography,\n} from '@material-ui/core';\nimport { Link } from 'react-router-dom';\nimport InstagramIcon from '@material-ui/icons/Instagram';\nimport FacebookIcon from '@material-ui/icons/Facebook';\nimport { ShoppingCart } from '@material-ui/icons';\nimport useStyles from './styles';\nimport logo from '../../assets/logosandpics/REKD.JPG';\n\nconst Navbar = ({ totalItems }) => {\n  const classes = useStyles();\n\n  return (\n    <>\n      <AppBar position=\"fixed\" className={classes.appBar} color=\"inherit\">\n        <Toolbar>\n          <Typography\n            component={Link}\n            to=\"/\"\n            variant=\"h6\"\n            className={classes.title}\n            color=\"inherit\"\n          >\n            <img\n              src={logo}\n              alt=\"Rekd the game\"\n              height=\"25px\"\n              className={classes.image}\n            />\n          </Typography>\n          <div className={classes.socialmediabutton}>\n            <Link\n              to={{ pathname: 'https://www.instagram.com/rekdthegame/' }}\n              target=\"_blank\"\n            >\n              <InstagramIcon />\n            </Link>\n            <Link\n              to={{ pathname: 'https://www.facebook.com/rekdthegame/' }}\n              target=\"_blank\"\n            >\n              <FacebookIcon />\n            </Link>\n          </div>\n          <div className={classes.buttonCart}>\n            <IconButton\n              component={Link}\n              to=\"/cart\"\n              aria-label=\"Show cart items\"\n              color=\"inherit\"\n            >\n              <Badge badgeContent={totalItems} color=\"secondary\">\n                <ShoppingCart />\n              </Badge>\n            </IconButton>\n          </div>\n        </Toolbar>\n      </AppBar>\n      {/* <div className={classes.toolbarMargin} /> */}\n    </>\n  );\n};\n\nexport default Navbar;\n","/Users/jesper/Desktop/rekdthegame/src/components/Cart/Cart.jsx",[],"/Users/jesper/Desktop/rekdthegame/src/components/Hero/Hero.jsx",["127"],"import React from 'react';\nimport useStyles from './styles';\nimport {\n  Card,\n  CardActions,\n  CardActionArea,\n  CardMedia,\n  CardContent,\n  Typography,\n} from '@material-ui/core';\nimport pic1 from '../../assets/logosandpics/IMG-1807.JPG';\nimport pic2 from '../../assets/logosandpics/IMG-1786.JPG';\nimport pic3 from '../../assets/logosandpics/IMG-1796.JPG';\n// import Button from '@material-ui/core/Button';\n\nconst Hero = () => {\n  const classes = useStyles();\n  return (\n    <>\n      <div className={classes.heroContainer}>\n        <Card className={classes.product}>\n          <CardActionArea className={classes.productCard}>\n            <CardMedia\n              className={classes.productCardImage}\n              component=\"img\"\n              alt=\"image 1\"\n              src={pic1}\n              title=\"image 1\"\n            />\n            <CardContent className={classes.productContent}>\n              <Typography\n                gutterBottom\n                variant=\"h5\"\n                component=\"h2\"\n                style={{ textAlign: 'center' }}\n              >\n                <h2>Hilarious Gameplay</h2>\n              </Typography>\n              <Typography variant=\"body2\" color=\"textSecondary\" component=\"p\">\n                Every square will lead to a different hysterical action for one\n                or more players. From Challenges that play like mini-games to\n                get ahead, to Punishments that will have your friends in\n                stitches! And don’t forget the Specials, which can completely\n                change the game. Every roll will bring excitement and humor!\n              </Typography>\n            </CardContent>\n          </CardActionArea>\n        </Card>\n\n        <Card className={classes.product}>\n          <CardActionArea className={classes.productCard}>\n            <CardMedia\n              className={classes.productCardImage}\n              component=\"img\"\n              alt=\"image 2\"\n              src={pic2}\n              title=\"image 2\"\n            />\n            <CardContent className={classes.productContent}>\n              <Typography\n                gutterBottom\n                variant=\"h5\"\n                component=\"h2\"\n                style={{ textAlign: 'center' }}\n              >\n                <h2>Get Competitive</h2>\n              </Typography>\n              <Typography variant=\"body2\" color=\"textSecondary\" component=\"p\">\n                The road to the finish is a dangerous one. Your first enemy is\n                the board, but your most deadly foe will be your friends.\n                Challenges will have you taking sides, feuds will be created and\n                Specials will awaken the competitive spirits in the room. Just\n                remember: taking that winning selfie is everything.\n              </Typography>\n            </CardContent>\n          </CardActionArea>\n        </Card>\n\n        <Card className={classes.product}>\n          <CardActionArea className={classes.productCard}>\n            <CardMedia\n              className={classes.productCardImage}\n              component=\"img\"\n              alt=\"image 3\"\n              src={pic3}\n              title=\"image 3\"\n            />\n            <CardContent className={classes.productContent}>\n              <Typography\n                gutterBottom\n                variant=\"h5\"\n                component=\"h2\"\n                style={{ textAlign: 'center' }}\n              >\n                <h2>Play Your Way</h2>\n              </Typography>\n              <Typography variant=\"body2\" color=\"textSecondary\" component=\"p\">\n                The game lets you make your own choices - and live with the\n                consequences. You can take the treacherous shortcuts and try to\n                get ahead, at your own peril. You may be faced with the\n                opportunity to send a player back to the start, creating a\n                rivalry. You can decide where to send drinks and who to call out\n                on Specials. It’s up to you.\n              </Typography>\n            </CardContent>\n          </CardActionArea>\n        </Card>\n      </div>\n    </>\n  );\n};\n\nexport default Hero;\n","/Users/jesper/Desktop/rekdthegame/src/components/Products/Product/styles.js",[],"/Users/jesper/Desktop/rekdthegame/src/components/Reviews/Reviews.jsx",["128","129","130","131"],"/Users/jesper/Desktop/rekdthegame/src/components/CheckoutForm/Review.jsx",["132"],"/Users/jesper/Desktop/rekdthegame/src/components/Reviews/styles.js",[],{"ruleId":"133","severity":1,"message":"134","line":49,"column":6,"nodeType":"135","endLine":49,"endColumn":12,"suggestions":"136"},{"ruleId":"137","replacedBy":"138"},{"ruleId":"139","replacedBy":"140"},{"ruleId":"137","replacedBy":"141"},{"ruleId":"139","replacedBy":"142"},{"ruleId":"143","severity":1,"message":"144","line":1,"column":21,"nodeType":"145","messageId":"146","endLine":1,"endColumn":35},{"ruleId":"143","severity":1,"message":"147","line":3,"column":17,"nodeType":"145","messageId":"146","endLine":3,"endColumn":25},{"ruleId":"137","replacedBy":"148"},{"ruleId":"139","replacedBy":"149"},{"ruleId":"137","replacedBy":"150"},{"ruleId":"139","replacedBy":"151"},{"ruleId":"137","replacedBy":"152"},{"ruleId":"139","replacedBy":"153"},{"ruleId":"143","severity":1,"mes